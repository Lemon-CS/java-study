<mapper namespace="user">
<!--    com.lemon.dao.IUserDao-->

    <!--sql的唯一标识：namespace.id来组成 ： statementId-->
    <select id="selectList" resultType="com.lemon.pojo.User" >
        select * from user
    </select>


    <!--
        User user = new User()
        user.setId(1);
        user.setUsername("zhangsan")
    -->
    <select id="selectOne" resultType="com.lemon.pojo.User" paramterType="com.lemon.pojo.User">
        select * from user where id = #{id} and username = #{username}
    </select>


</mapper>